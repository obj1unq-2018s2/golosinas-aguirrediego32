import golosinas.*
describe "test caramelo"{
	
	test"peso inicial"{
		assert.equals(5,caramelo.peso())
	}
	
	test"precio"{
		assert.equals(1,caramelo.precio())
	}
	
	test "gusto"{
		assert.equals("frutilla",caramelo.gusto())
	}
	
	test"peso"{
		assert.equals(5,caramelo.peso())
	}
	test "mordisco"{
		caramelo.mordisco()
		assert.equals(4,caramelo.peso())
	}
	test"segundo mordisco"{
		caramelo.mordisco()
		caramelo.mordisco()
		assert.equals(3,caramelo.peso())
	}
}